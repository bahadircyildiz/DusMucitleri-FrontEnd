{{#if services}}
<div class="col-md-9">
    <div class="gt_cd_outer_wrap default_width">
        <figure>
            <img src="{{services.image}}" alt="{{services.title}}">
        </figure>
        <div class="ct_blog_detail_top">
            <h5>{{services.title}}</h5>
            <ul>
                {{!--{{#getInstructor courses.instructorID}}--}}
                {{!--<li>--}}
                {{!--    <span>{{{title}}}</span>--}}
                {{!--    <h6>{{{name}}}</h6>--}}
                {{!--</li>--}}
                {{!--{{/getInstructor}}--}}
                <li>
                    <span>Kategori</span>
                    <h6>{{#getByID services.categoryID "servicecat"}} {{title}} {{/getByID}}</h6>
                </li>
                <li>
                    <span>Ayrıntılı bilgi için 0544 370 35 15 – 0232 370 35 15 ‘i arayınız</span>
                </li>
                {{!--<li>--}}
                {{!--    <span>Review</span>--}}
                {{!--    <ul class="gt_rating_start">--}}
                {{!--        <li>--}}
                {{!--            <a href="#"><i class="fa fa-star"></i></a>--}}
                {{!--            <a href="#"><i class="fa fa-star"></i></a>--}}
                {{!--            <a href="#"><i class="fa fa-star"></i></a>--}}
                {{!--            <a href="#"><i class="fa fa-star"></i></a>--}}
                {{!--            <a href="#"><i class="fa fa-star-o"></i></a>--}}
                {{!--        </li>--}}
                {{!--    </ul>--}}
                {{!--</li>--}}
            </ul>
        </div>
        <div class="gt_course_price">
            {{!--<span> Ayrıntılı bilgi için 0544 370 35 15 – 0232 370 35 15 ‘i arayınız</span>--}}
            {{!--<a href="#">Kursa Katıl</a>--}}
        </div>
        {{{services.body}}}
    </div>
    <div class="gt_class_spec default_width">
        <ul>
            {{#each services.specs}}
            <li>
                <span><i class="{{{icon}}}"></i></span>
                <div class="cls_spec_des">
                    <p>{{{key}}}</p>
                    <h6>{{{value}}}</h6>
                </div>
            </li>
            {{/each}}
        </ul>
    </div>
</div>
{{/if}}