<!--Header Wrap Start-->
    <header>
        <div class="gt_top3_wrap default_width">
            <div class="container">
                <div class="gt_top3_scl_icon">
                    <ul class="gt_hdr3_scl_icon">
                        <li><a href="https://www.facebook.com/{{settings.facebook}}"><i class="fa fa-facebook"></i></a></li>
                        <li><a href="https://www.instagram.com/{{settings.instagram}}"><i class="fa fa-instagram"></i></a></li>
                    </ul>
                </div>
                <div class="gt_hdr_3_ui_element">
                    <ul>
                        {{#each settings.phones}}
                        <li><i class="fa fa-phone"></i><a href="#">{{{text}}}</a></li>
                        {{/each}}
                        {{#each settings.emails}}
                        <li><i class="fa fa-envelope-o"></i><a href="mailto:{{{text}}}">{{{text}}}</a></li>
                        {{/each}}
                    </ul>
                </div>
                {{!--<div class="gt_hdr3_search">--}}
                {{!--    <form>--}}
                {{!--        <input type="search" placeholder="Enter your search...">--}}
                {{!--        <button><i class="fa fa-search"></i></button>--}}
                {{!--    </form>--}}
                {{!--</div>--}}
            </div>
        </div>
        <div class="gt_top3_menu default_width single-page">
            <div class="container">
                <div class="gt-logo">
                    <a href="/"><img src="/{{settings.logo}}" alt="{{settings.siteTitle}} - {{settings.siteSubtitle}}"></a>
                </div>
                <nav class="gt_hdr3_navigation">
                    <!-- Responsive Buttun -->
                    <a class="navbar-btn collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </a>   
                    <!-- Responsive Buttun -->
                    <ul class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                        {{#each navigation}}
                            {{!--{{#if_eq @index 0}}--}}
                            {{!--<li class="active">--}}
                            {{!--{{else}}--}}
                            {{!--<li>--}}
                            {{!--{{/if_eq}}--}}
                        <li><a href="{{this.link}}">{{this.title}}</a>
                            {{#if children}} 
                                <ul> 
                                {{#each children}}
                                    <li><a href="{{this.link}}">{{this.title}}</a></li>
                                {{/each}}
                                </ul>
                            {{/if}}
                        </li>
                        {{/each}}
                    </ul>
                </nav>
            </div>
        </div>
    </header>
    <!--Header Wrap End-->