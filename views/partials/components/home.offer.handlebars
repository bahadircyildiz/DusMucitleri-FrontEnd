<!--Offer Wrap start-->
<section class="gt_wht_offer_bg" id="services">
    <div class="container">
    	<div class="gt_hdg_1">
            <h3>{{contents.home.offers.title}}</h3>
            {{{contents.home.offers.body}}}
            <span><img src="static/images/hdg-01.png" alt=""></span>
        </div>
        <!--What We Offer 2 Wrap Start-->
        <div class="row">
            {{#each offers}}
            <div class="col-md-4 col-sm-6">
                <div class="gt_wht_offer_wrap mb">
                    <i class="{{{icon}}} offers_bg_{{@index}}"></i>
                    <div class="gt_wht_offer_des">
                        <h5><a href="#">{{{title}}}</a></h5>
                        <span class="bg_offers_{{@index}}"></span>
                        <p>{{{description}}}</p>
                    </div>
                </div>
            </div>
            {{/each}}
        </div>
        <!--What We Offer 2 Wrap End-->
    </div>
</section>
<!--offer Wrap End-->